{
  "name": "genSound",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/usr/src/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.isort",
        "ms-python.pylint",
        "ms-python.flake8"
      ],
      "settings": {
        "python.formatting.provider": "black",
        "python.formatting.blackArgs": ["--line-length", "88"],
        "editor.formatOnSave": true,
        "git.path": "/usr/bin/git"
      }
    }
  }
}
